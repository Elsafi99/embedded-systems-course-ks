#include <stdio.h>

struct data{
	char* Name;
	int ID;
};


int main(void) {

	struct data employee_1={"AHMED",23147};
	struct data employee_2={"ALEX",1002};
	struct data employee_3={"NOOR",45789};




	return 0;
}
