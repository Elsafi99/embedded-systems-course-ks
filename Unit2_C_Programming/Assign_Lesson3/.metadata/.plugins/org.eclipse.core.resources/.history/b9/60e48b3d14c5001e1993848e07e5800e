#include <stdio.h>
#include <stdlib.h>

int main(void) {
	    float num1=0.0;
		float num2=0.0;
		float mult=0.0;

		printf(" Enter Two Float Numbers = ");
		fflush(stdin);fflush(stdout);
		scanf("%1f %1f",&num1,&num2);
		mult=num1*num2;
		printf("\r\n Product=%6f",mult);
		return EXIT_SUCCESS;
}
